#include <stdio.h>
#include <stdlib.h>

int main()
{
    int size,*arr,num;
    printf("Enter size of array\n");
    scanf("%d",&size);
    arr=(int*)calloc(size,sizeof(int));
    printf("Enter Elements\n");
    for(int i=0;i<size;i++)
    {
        scanf("%d",&arr[i]);
    }
    printf("Enter num to search\n");
    scanf("%d",&num);
    for(int i=0;i<size;i++)
	{
		if(num==arr[i])
		{
			printf("Location : %d\n",i+1);
			break;
		}
	}
    return 0;
}
